{
  "name": "bgregory-dev",
  "version": "1.0.0",
  "description": "Brett's resume website",
  "private": true,
  "type": "commonjs",
  "engines": {
    "node": "^16.0.0",
    "npm": "^6.13.4"
  },
  "scripts": {
    "build:client": "node --optimize_for_size --gc_interval=100 node_modules/webpack/bin/webpack.js --config config/webpack-client-config.js",
    "build:server": "node node_modules/webpack/bin/webpack.js --config config/webpack-server-config.js",
    "dev:server": "nodemon -r tsconfig-paths/register -e js,ts,tsx server/server-entry.ts --exec ts-node",
    "dev:client": "webpack --progress --watch --config ./config/webpack-client-config.js"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/glob": "^7.2.0",
    "@types/react": "^17.0.34",
    "@types/react-dom": "^17.0.11",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^3.1.1",
    "mini-css-extract-plugin": "^2.4.3",
    "postcss-loader": "^6.2.0",
    "prettier": "^2.4.1",
    "sass-loader": "^12.3.0",
    "terser-webpack-plugin": "^5.2.4",
    "ts-loader": "^9.2.6",
    "typescript": "^4.4.4",
    "webpack": "^5.61.0",
    "webpack-cli": "^4.9.1"
  },
  "dependencies": {
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "glob": "^7.2.0",
    "nodemon": "^2.0.14",
    "pug": "^3.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.11.0"
  }
}
